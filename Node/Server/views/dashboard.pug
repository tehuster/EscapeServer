extends layout.pug

block title
    | Dashboard

block scripts     

block content   
    .kt-portlet
        .kt-portlet__body.kt-portlet__body--fit
            .row.row-no-padding.row-col-separator-lg
                .col-md-12.col-lg-6.col-xl-3
                    // begin::Total Profit
                    .kt-widget24
                        .kt-widget24__details
                            .kt-widget24__info
                                h4.kt-widget24__title
                                    | St. Jan
                                span.kt-widget24__desc
                                    | All Puzzles
                            span.kt-widget24__stats.kt-font-brand
                                | Last Event
                        .progress.progress--sm
                            .progress-bar.kt-bg-brand(role="progressbar" style="width: 78%;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100")
                        .kt-widget24__action
                            span.kt-widget24__change
                                | Progress
                            span.kt-widget24__number
                                | 78%
                    // end::Total Profit
                .col-md-12.col-lg-6.col-xl-3
                    // begin::New Feedbacks
                    .kt-widget24
